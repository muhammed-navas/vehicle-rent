# This is an example .env file - you should update your actual .env file with these values
# adjusted for your PostgreSQL setup

# PostgreSQL Connection URL format: postgresql://USERNAME:PASSWORD@HOST:PORT/DATABASE?schema=SCHEMA
# Replace the values with your actual PostgreSQL connection details
DATABASE_URL="postgresql://postgres:your_password@localhost:5432/vehicle_rent?schema=public"

# Server Port
PORT=5000

# CORS Origin - update this if your frontend is running on a different URL
CORS_ORIGIN="http://localhost:3000"

# Node Environment
NODE_ENV="development"

# Note: After updating your .env file with the correct values,
# restart your server and the database connection should work
